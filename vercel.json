{
    "version": 2,
    "builds": [
      {
        "src": "index.js",  
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/verifyLogin",
        "dest": "/index.js"  
      },
      {
        "src": "/updatelocation",
        "dest": "/index.js" 
      },
      {
        "src": "/register",
        "dest": "/index.js" 
      },
      {
        "src": "/verifytoken",
        "dest": "/index.js" 
      },
      {
        "src": "/(.*)",
        "dest": "/index.js" 
      }
    ]
  }
  